	//loop 3 vezes
	for(i=3;i<3;i++)
		printf("Placa: ")
		scanf(%s, plh,placa)
	
	//pega placa
	scanf("%s",&dado.placa);
	//guarda na pilha
	push(plh, dado);
	//
}

int main(){
	
