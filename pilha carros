#include <stdio.h>
#include <stdlib.h>

typedef struct elemento{
	char placa[24];
} t_elemento;

typedef struct pilha{
	t_elemento vetor[3];
	int topo;
}t_pilha;

int push(t_pilha * pilha, t_elemento valor){
	
	//if(isCheia(pilha))
	//	return 0;//erro
		
	pilha->vetor[++pilha->topo] = valor;
	
	return 1;//sucesso
}

